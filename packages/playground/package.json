{
  "name": "@studiometa/playground",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/browser.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./browser": {
      "import": "./dist/browser.js"
    }
  },
  "scripts": {
    "watch": "node scripts/watch.js",
    "prebuild": "rm -rf dist",
    "build": "npm run build:esbuild && npm run build:types",
    "build:esbuild": "node scripts/build.js",
    "build:types": "tsc --build tsconfig.build.json"
  },
  "dependencies": {
    "@studiometa/js-toolkit": "^2.12.0",
    "@studiometa/webpack-config": "^1.1.3",
    "@studiometa/webpack-config-preset-prototyping": "^5.3.0",
    "emmet-monaco-es": "^5.3.0",
    "esbuild-wasm": "^0.20.0",
    "fast-glob": "^3.3.2",
    "fflate": "^0.8.2",
    "monaco-editor": "^0.45.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "tailwindcss": "^3.4.1"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
